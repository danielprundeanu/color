        "bottom": {
          "$type": "spacing",
          "$value": "{space.0}",
          "$description": "Carousel Medium bottom padding."
        }
      },
      "arrow": {
        "size": {
          "$type": "sizing",
          "$value": "{size.300}",
          "$description": "Arrows medium size"
        }
      },
      "border": {
        "top": {
          "$type": "borderWidth",
          "$value": "{border.none}"
        },
        "right": {
          "$type": "borderWidth",
          "$value": "{border.none}"
        },
        "bottom": {
          "$type": "borderWidth",
          "$value": "{border.none}"
        },
        "left": {
          "$type": "borderWidth",
          "$value": "{border.none}"
        }
      }
    },
