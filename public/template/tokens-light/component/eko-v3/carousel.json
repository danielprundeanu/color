      }
    },
    "arrow": {
      "fill": {
        "default": {
          "$type": "color",
          "$value": "{surface.strong}"
        },
        "hover": {
          "$type": "color",
          "$value": "{surface.default}"
        },
        "focus": {
          "$type": "color",
          "$value": "{surface.strongest}"
        }
      },
      "border": {
        "default": {
          "$type": "color",
          "$value": "{surface.strong}"
        },
        "hover": {
          "$type": "color",
          "$value": "{surface.default}"
        },
        "focus": {
          "$type": "color",
          "$value": "{surface.strongest}"
        },
        "width": {
          "$type": "borderWidth",
          "$value": "{border.xs}"
        }
      },
      "on": {
        "fill": {
          "default": {
            "$type": "color",
            "$value": "{on.surface.head}"
          },
          "hover": {
            "$type": "color",
            "$value": "{on.surface.head}"
          },
          "focus": {
            "$type": "color",
            "$value": "{on.surface.head}"
          }
        }
      },
      "default": {
        "$type": "boxShadow",
        "$value": {
          "x": "-3",
          "y": "2.25",
          "blur": "4.88",
          "spread": "0",
          "color": "transparent",
          "type": "dropShadow"
        }
