{
  "carousel": {
    "md": {
      "slide": {
        "space": {
          "left": {
            "$type": "spacing",
            "$value": "{space.0}",
            "$description": "Carousel Medium slide padding left."
          },
          "right": {
            "$type": "spacing",
            "$value": "{space.100}",
            "$description": "Carousel Medium slide padding right."
          }
        }
      },
      "offset": {
        "left": {
          "$type": "spacing",
          "$value": "{space.0}",
          "$description": "Carousel Medium left offset."
        },
        "right": {
          "$type": "spacing",
          "$value": "{space.700}",
          "$description": "Carousel Medium right offset."
        }
      },
      "space": {
        "top": {
          "$type": "spacing",
