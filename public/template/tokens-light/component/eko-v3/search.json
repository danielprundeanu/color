{
  "primary-search": {
    "max-width": {
      "$type": "sizing",
      "$value": "600px"
    },
    "height": {
      "$type": "sizing",
      "$value": "{size.500}",
      "$description": "Top Navigation / Header search height."
    },
    "icon": {
      "size": {
        "$type": "sizing",
        "$value": "{size.300}",
        "$description": "Primary Search icon size."
      }
    },
    "gap": {
      "$type": "spacing",
      "$value": "{space.100}",
      "$description": "Top Navigation / Header search gap."
    },
    "space": {
      "top": {
        "$type": "spacing",
        "$value": "{space.100}",
        "$description": "Top Navigation / Header Search input top padding."
      },
      "right": {
        "$type": "spacing",
        "$value": "{space.150}",
        "$description": "Top Navigation / Header Search input right padding."
      },
      "bottom": {
        "$type": "spacing",
        "$value": "{space.100}",
        "$description": "Top Navigation / Header Search input bottom padding."
      },
      "left": {
        "$type": "spacing",
        "$value": "{space.150}",
        "$description": "Top Navigation / Header Search input left padding."
      }
    },
    "radius": {
      "$type": "borderRadius",
      "$value": "{radius.md}",
      "$description": "Top Navigation / Header search input border radius."
    },
    "border": {
      "all": {
        "$type": "borderWidth",
        "$value": "{size.10}",
        "$description": "Top Navigation / Header search input border width."
      },
      "default": {
        "$type": "color",
        "$value": "{transparent}",
        "$description": "Primary Search default border."
      },
      "hover": {
        "$type": "color",
        "$value": "{primary.border.default}",
        "$description": "Primary Search hover border."
      },
      "focus": {
        "$type": "color",
        "$value": "{primary.fill.default}",
        "$description": "Primary Search focus border."
      }
    },
    "typo": {
      "$type": "typography",
      "$value": {
        "fontFamily": "{font-family.200}",
        "fontSize": "{font-size.90}",
        "fontWeight": "{font-weight.600}",
        "lineHeight": "{line-height.md} * 100%"
      },
      "$description": "Primary search input label typo."
    },
    "fill": {
      "default": {
        "$extensions": {
          "studio.tokens": {
            "modify": {
              "type": "alpha",
              "value": "0.1",
              "space": "srgb",
              "color": "{neutral.40} "
            }
          }
        },
        "$type": "color",
        "$value": "{white.100}",
        "$description": "Primary Search default fill."
      },
      "hover": {
        "$type": "color",
        "$value": "{surface.default}",
        "$description": "Primary Search hover fill."
      },
      "focus": {
        "$type": "color",
        "$value": "{surface.default}",
        "$description": "Primary Search focus fill."
      }
    },
    "on": {
      "fill": {
        "default": {
          "$extensions": {
            "studio.tokens": {
              "modify": {
                "type": "alpha",
                "value": "0.6",
                "space": "srgb"
              }
            }
          },
          "$type": "color",
          "$value": "{white.100}",
          "$description": "Primary Search default on.fill."
        },
        "hover": {
          "$type": "color",
          "$value": "{on.surface.head}",
          "$description": "Primary Search hover on.fill."
        },
        "focus": {
          "$type": "color",
          "$value": "{on.surface.body}",
          "$description": "Primary Search focus on.fill."
        }
      }
    },
    "shadow": {
      "default": {
        "$type": "boxShadow",
        "$value": {
          "x": "0",
          "y": "0",
          "blur": "0",
          "spread": "0",
          "color": "transparent",
          "type": "dropShadow"
        },
        "$description": "Primary Search default shadow."
      },
      "hover": {
        "$type": "boxShadow",
        "$value": {
          "x": "0",
          "y": "0",
          "blur": "0",
          "spread": "0",
          "color": "transparent",
          "type": "dropShadow"
        },
        "$description": "Primary Search hover shadow."
      },
      "focus": {
        "$type": "boxShadow",
        "$value": {
          "x": "0",
          "y": "0",
          "blur": "0",
          "spread": "0",
          "color": "transparent",
          "type": "dropShadow"
        },
        "$description": "Primary Search focus shadow."
      }
    }
  },
