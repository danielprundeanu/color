{
  "icon": {
    "info": {
      "fill": {
        "default": {
          "$type": "color",
          "$value": "{on.surface.head}"
        },
        "active": {
          "$type": "color",
          "$value": "{on.surface.primary.active}"
        }
      },
      "shadow": {
        "$type": "boxShadow",
        "$value": {
          "x": "0",
          "y": "0",
          "blur": "0",
          "spread": "0",
          "color": "{transparent}",
          "type": "dropShadow"
        }
      }
    }
  }
}