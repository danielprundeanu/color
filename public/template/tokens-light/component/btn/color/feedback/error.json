{
  "aux-btn": {
    "fbk": {
      "shadow": {
        "xs": {
          "default": {
            "$type": "boxShadow",
            "$value": [
              {
                "x": "{shadow.base.xs.x}",
                "y": "{shadow.base.xs.y}",
                "blur": "{shadow.base.xs.blur}",
                "spread": "{shadow.base.xs.spread}",
                "color": "{btn.fbk.shadow.fill.default}",
                "type": "dropShadow"
              }
            ],
            "$description": "Adding transparency for the hex value as you can see here: https://gist.github.com/lopspower/03fb1cc0ac9f32ef38f4"
          },
          "active": {
            "$type": "boxShadow",
            "$value": [
              {
                "x": "{shadow.base.xs.x}",
                "y": "{shadow.base.xs.y}",
                "blur": "{shadow.base.xs.blur}",
