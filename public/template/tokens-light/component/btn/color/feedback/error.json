          }
        }
      },
      "gradient": {
        "default": {
          "start": {
            "$type": "color",
            "$value": "{error.fill.default}"
          },
          "end": {
            "$type": "color",
            "$value": "{error.fill.active}"
          }
        },
        "active": {
          "start": {
            "$type": "color",
            "$value": "{error.fill.active}"
          },
          "end": {
            "$type": "color",
            "$value": "{negative.100}"
          }
        }
      }
    }
  },
  "btn": {
    "fbk": {
      "fill": {
        "default": {
          "$type": "color",
          "$value": "linear-gradient(224deg, {aux-btn.fbk.gradient.default.start} 0%, {aux-btn.fbk.gradient.default.end} 80%)"
        },
        "active": {
          "$type": "color",
          "$value": "linear-gradient(224deg, {aux-btn.fbk.gradient.active.start} 0%, {aux-btn.fbk.gradient.active.end} 80%)"
        }
      },
      "border": {
        "default": {
          "$type": "color",
          "$value": "{error.fill.default}"
        },
        "active": {
          "$type": "color",
          "$value": "{error.fill.active}"
        }
      },
      "on": {
        "fill": {
          "default": {
            "$type": "color",
            "$value": "{error.on.fill.default}"
          },
          "active": {
            "$type": "color",
            "$value": "{success.on.fill.default}"
          }
        }
      },
      "shadow": {
        "fill": {
          "default": {
            "$type": "color",
            "$value": "{negative.130}4D",
            "$description": "Token creat pentru a putea face swap intre 2 teme și pentru a schimba referința de culoare a style-ului de shadow din Figma."
          },
          "active": {
            "$type": "color",
            "$value": "{negative.130}4D",
            "$description": "Token creat pentru a putea face swap intre 2 teme și pentru a schimba referința de culoare a style-ului de shadow din Figma."
          }
        }
      }
    }
