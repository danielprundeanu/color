{
  "bottom-nav": {
    "item": {
      "fill": {
        "default": {
          "$type": "color",
          "$value": "{surface.strong}"
        },
        "active": {
          "$type": "color",
          "$value": "{primary.fill.active}"
        }
      },
      "on": {
        "surface": {
          "label": {
            "default": {
              "$type": "color",
              "$value": "{on.surface.head}"
            },
            "active": {
              "$type": "color",
              "$value": "{on.surface.primary.head}"
            }
          },
          "icon": {
            "default": {
              "$type": "color",
              "$value": "{on.surface.label}"
            },
            "active": {
              "$type": "color",
              "$value": "{on.surface.primary.head}"
            }
          }
        },
        "fill": {
          "icon": {
            "default": {
              "$type": "color",
              "$value": "{on.surface.label}"
            },
            "active": {
              "$type": "color",
              "$value": "{primary.on.fill.default}"
            }
          },
          "label": {
            "default": {
              "$type": "color",
              "$value": "{on.surface.label}"
            },
            "active": {
              "$type": "color",
              "$value": "{primary.on.fill.default}"
            }
          }
        }
      },
      "border": {
        "default": {
          "$type": "color",
          "$value": "{transparent}"
        },
        "active": {
          "$type": "color",
          "$value": "{primary.fill.active}"
        }
      }
    },
    "list": {
      "fill": {
        "default": {
          "$type": "color",
          "$value": "{surface.default}"
        }
      },
      "border": {
        "default": {
          "$type": "color",
          "$value": "{border.default}"
        }
      },
      "separator": {
        "$type": "color",
        "$value": "{border.separator}"
      }
    },
    "shadow": {
      "$type": "color",
      "$value": "{black.10}"
    }
  },
  "aux-bottom-nav": {
    "shadow": {
      "$type": "boxShadow",
      "$value": {
        "x": "{shadow.base.sm.x}",
        "y": "{shadow.base.sm.y}",
        "blur": "{shadow.base.sm.blur}",
        "spread": "{shadow.base.sm.spread}",
        "color": "{bottom-nav.shadow}",
        "type": "dropShadow"
      }
    }
  }
}