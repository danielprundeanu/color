{
  "timer": {
    "fill": {
      "$type": "color",
      "$value": "{surface.strong}",
      "$description": "Timer fill color."
    },
    "border": {
      "default": {
        "$type": "color",
        "$value": "{border.default}",
        "$description": "Timer border color."
      }
    },
    "on": {
      "fill": {
        "head": {
          "$type": "color",
          "$value": "{on.surface.head}",
          "$description": "Timer title color."
        },
        "body": {
          "$type": "color",
          "$value": "{on.surface.head}",
          "$description": "Timer - item text color."
        },
        "label": {
          "$extensions": {
            "studio.tokens": {
              "modify": {
                "type": "alpha",
                "value": "0.8",
                "space": "srgb"
              }
            }
          },
          "$type": "color",
          "$value": "{on.surface.label}",
          "$description": "TImer - unit text color."
        },
        "icon": {
          "$type": "color",
          "$value": "{on.surface.label}"
        }
      },
      "surface": {
        "head": {
          "$type": "color",
          "$value": "{on.surface.head}",
          "$description": "Timer title color."
        },
        "body": {
          "$type": "color",
          "$value": "{on.surface.head}",
          "$description": "Timer - item text color."
        },
        "label": {
          "$extensions": {
            "studio.tokens": {
              "modify": {
                "type": "alpha",
                "value": "0.8",
                "space": "srgb"
              }
            }
          },
          "$type": "color",
          "$value": "{on.surface.label}",
          "$description": "TImer - unit text color."
        },
        "icon": {
          "$type": "color",
          "$value": "{on.surface.label}"
        }
      }
    },
    "shadow": {
      "fill": {
        "$type": "color",
        "$value": "{black.10}"
      }
    },
    "warning": {
      "fill": {
        "$type": "color",
        "$value": "{warning.surface.default}",
        "$description": "Timer fill color."
      },
      "border": {
        "default": {
          "$type": "color",
          "$value": "{warning.border.default}",
          "$description": "Timer border color."
        }
      },
      "on": {
        "fill": {
          "head": {
            "$type": "color",
            "$value": "{on.surface.warning.head}",
            "$description": "Timer title color."
          },
          "body": {
            "$type": "color",
            "$value": "{on.surface.warning.head}",
            "$description": "Timer - item text color."
          },
          "label": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "alpha",
                  "value": "0.7",
                  "space": "srgb"
                }
              }
            },
            "$type": "color",
            "$value": "{on.surface.warning.head}",
            "$description": "TImer - unit text color."
          },
          "icon": {
            "$type": "color",
            "$value": "{warning.fill.active}"
          }
        },
        "surface": {
          "head": {
            "$type": "color",
            "$value": "{on.surface.warning.head}",
            "$description": "Timer title color."
          },
          "body": {
            "$type": "color",
            "$value": "{on.surface.warning.head}",
            "$description": "Timer - item text color."
          },
          "label": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "alpha",
