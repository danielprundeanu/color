      },
      "on": {
        "fill": {
          "head": {
            "$type": "color",
            "$value": "{error.on.surface.default}",
            "$description": "Timer title color."
          },
          "body": {
            "$type": "color",
            "$value": "{on.surface.error.head}",
            "$description": "Timer - item text color."
          },
          "label": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "alpha",
                  "value": "0.7",
