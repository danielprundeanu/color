          },
          "body": {
            "$type": "color",
            "$value": "{on.surface.error.head}",
            "$description": "Timer - item text color."
          },
          "label": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "alpha",
                  "value": "0.7",
                  "space": "srgb"
                }
              }
            },
            "$type": "color",
            "$value": "{on.surface.error.head}",
            "$description": "TImer - unit text color."
          },
          "icon": {
            "$type": "color",
            "$value": "{on.surface.error.head}"
          }
        }
      },
      "shadow": {
        "fill": {
          "$type": "color",
          "$value": "{negative.90}26"
        }
      }
    }
  },
  "aux-timer": {
    "shadow": {
      "$type": "boxShadow",
      "$value": {
        "x": "{shadow.base.md.x}",
        "y": "{shadow.base.md.y}",
        "blur": "{shadow.base.md.blur}",
        "spread": "{shadow.base.md.spread}",
        "color": "{timer.shadow.fill}",
        "type": "dropShadow"
      }
    },
    "warning": {
      "shadow": {
        "$type": "boxShadow",
        "$value": {
          "x": "{shadow.base.md.x}",
          "y": "{shadow.base.md.y}",
          "blur": "{shadow.base.md.blur}",
          "spread": "{shadow.base.md.spread}",
