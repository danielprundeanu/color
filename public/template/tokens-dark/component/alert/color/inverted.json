{
  "alert": {
    "info": {
      "surface": {
        "$type": "color",
        "$value": "{informative.100}",
        "$description": "Info Alert surface color."
      },
      "fill": {
        "$type": "color",
        "$value": "{info.fill.default}",
        "$description": "Info Alert fill color. (used for feedback icon)."
      },
      "border": {
        "$type": "color",
        "$value": "{informative.80}",
        "$description": "Info Alert border color."
      },
      "on": {
        "surface": {
          "$type": "color",
          "$value": "{informative.20}",
          "$description": "Info Alert on surface color."
        }
      },
      "shadow": {
        "$type": "boxShadow",
        "$value": {
          "x": "0",
          "y": "0",
          "blur": "0",
          "spread": "0",
          "color": "#000000",
          "type": "dropShadow"
        },
        "$description": "Info Alert shadow."
      }
    },
