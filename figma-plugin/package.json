{
  "name": "eko-token-sync-plugin",
  "version": "1.0.0",
  "description": "Bidirectional sync between Git tokens and Figma Variables",
  "scripts": {
    "build": "tsc && npm run bundle",
    "bundle": "esbuild ./code.js --bundle --outfile=dist/code.js --target=es2017",
    "watch": "tsc --watch",
    "dev": "concurrently \"npm run watch\" \"npm run bundle -- --watch\""
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.80.0",
    "@types/node": "^20.0.0",
    "esbuild": "^0.19.0",
    "typescript": "^5.2.0",
    "concurrently": "^8.2.0"
  },
  "dependencies": {}
}
