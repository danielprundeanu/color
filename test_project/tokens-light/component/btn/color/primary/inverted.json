          "start": {
            "$type": "color",
            "$value": "{primary-inverted.30}"
          },
          "end": {
            "$type": "color",
            "$value": "{primary-inverted.60}"
          }
        },
        "active": {
          "start": {
            "$type": "color",
            "$value": "{primary-inverted.60}"
          },
          "end": {
            "$type": "color",
            "$value": "{primary-inverted.90}"
