  "btn": {
    "fill": {
      "disabled": {
        "$type": "color",
        "$value": "{inverted.fill.disabled}"
      }
    },
    "border": {
      "disabled": {
        "$type": "color",
        "$value": "{inverted.fill.disabled}"
      }
    },
    "on": {
      "fill": {
        "disabled": {
          "$type": "color",
          "$value": "{inverted.on.fill.neutral.disabled}"
        }
      }
    },
    "primary": {
      "fill": {
        "default": {
          "$type": "color",
          "$value": "linear-gradient(224deg, {aux-btn.primary.gradient.default.start} 0%, {aux-btn.primary.gradient.default.end} 80%)"
        },
        "active": {
          "$type": "color",
          "$value": "linear-gradient(224deg, {aux-btn.primary.gradient.active.start} 0%, {aux-btn.primary.gradient.active.end} 80%)"
        }
      },
      "border": {
        "default": {
          "$type": "color",
          "$value": "{primary-inverted.30}"
        },
        "active": {
          "$type": "color",
          "$value": "{primary-inverted.50}"
        }
      },
      "on": {
        "fill": {
          "default": {
            "$type": "color",
            "$value": "{primary-inverted.130}"
          },
          "active": {
            "$type": "color",
            "$value": "{primary-inverted.130}"
          }
        }
      },
      "shadow": {
        "fill": {
          "default": {
            "$type": "color",
            "$value": "{primary-inverted.130}4D",
            "$description": "Token creat pentru a putea face swap intre 2 teme și pentru a schimba referința de culoare a style-ului de shadow din Figma."
          },
          "active": {
            "$type": "color",
            "$value": "{primary-inverted.130}4D",
            "$description": "Token creat pentru a putea face swap intre 2 teme și pentru a schimba referința de culoare a style-ului de shadow din Figma."
