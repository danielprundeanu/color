{
  "top-nav-ext": {
    "surface": {
      "$type": "color",
      "$value": "{white.100}",
      "$description": "Top nav extension - top surface color."
    },
    "on": {
      "surface": {
        "$type": "color",
        "$value": "{on.surface.body}",
        "$description": "On top extension surface color."
      }
    },
    "border": {
      "default": {
        "$type": "color",
        "$value": "{primary.fill.default}",
        "$description": "Top nav - top extension border color."
      }
    },
    "shadow": {
      "fill": {
        "$type": "color",
        "$value": "{primary.surface.strong}2D"
      }
    }
  },
  "aux-top-nav-ext": {
    "shadow": {
      "$type": "boxShadow",
      "$value": {
        "x": "{shadow.base.xs.x}",
        "y": "{shadow.base.xs.y}",
        "blur": "{shadow.base.xs.blur}",
        "spread": "{shadow.base.xs.spread}",
        "color": "{top-nav-ext.shadow.fill}",
        "type": "dropShadow"
      }
    }
  }
}