      "border": {
        "$extensions": {
          "studio.tokens": {
            "modify": {
              "type": "alpha",
              "value": "0.3",
              "space": "srgb"
            }
          }
        },
        "$type": "color",
        "$value": "{white.100}"
      },
      "fill": {
        "$extensions": {
          "studio.tokens": {
            "modify": {
              "type": "alpha",
              "value": "0.15",
