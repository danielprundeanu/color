                }
              }
            },
            "$type": "color",
            "$value": "{grey.fill.disabled}",
            "$description": "Toggle -> Switch border color. (the circle and the active pill)"
          }
        }
      }
    },
    "on": {
      "surface": {
        "default": {
          "$type": "color",
          "$value": "{on.surface.label}"
        },
        "disabled": {
          "$type": "color",
          "$value": "{on.surface.disabled}"
        }
      }
    },
    "switch": {
      "fill": {
        "default": {
          "$type": "color",
          "$value": "{white.100}",
          "$description": "Toggle -> Switch fill color. (the circle and the active pill)"
        }
      },
      "border": {
        "default": {
          "$type": "color",
          "$value": "{white.100}",
          "$description": "Toggle -> Switch border color. (the circle and the active pill)"
        }
      }
    },
